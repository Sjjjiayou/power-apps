<?xml version="1.0" encoding="utf-8" ?>
<manifest>
  <control namespace="SampleNamespace" constructor="AnnualReport" version="1.0.6" display-name-key="AnnualReport" description-key="AnnualReport description" control-type="standard">
    <!--external-service-usage node declares whether this 3rd party PCF control is using external service or not, if yes, this control will be considered as premium and please also add the external domain it is using.
    If it is not using any external service, please set the enabled="false" and DO NOT add any domain below. The "enabled" will be false by default.
    Example1:
      <external-service-usage enabled="true">
        <domain>www.Microsoft.com</domain>
      </external-service-usage>
    Example2:
      <external-service-usage enabled="false">
      </external-service-usage>
    -->
    <external-service-usage enabled="false">
      <!--UNCOMMENT TO ADD EXTERNAL DOMAINS
      <domain></domain>
      <domain></domain>
      -->
    </external-service-usage>
    <!-- dataset node represents a set of entity records on CDS; allow more than one datasets -->
    <data-set name="records" display-name-key="sampleDataSetProperty">
      <property-set name="RecordsIndicator" display-name-key="RecordsIndicator_Key" description-key="DRecordsIndicator_Desc" of-type="SingleLine.Text" usage="bound" required="true" />
    </data-set>
    <data-set name="data" display-name-key="dataSetProperty">
      <property-set name="DataIndicator" display-name-key="DataIndicator_Key" description-key="DataIndicator_Desc" of-type="SingleLine.Text" usage="bound" required="true" />
    </data-set>
    <property name="Style1TopPosition" display-name-key="Property_Style1TopPosition" description-key="Property_Style1TopPosition" of-type="SingleLine.Text" usage="bound" required="true" default-value="150"/>
    <property name="Style1LeftPosition" display-name-key="Property_Style1LeftPosition" description-key="Property_Style1LeftPosition" of-type="SingleLine.Text" usage="bound" required="true" default-value="24"/>
    <property name="Style2TopPosition" display-name-key="Property_Style2TopPosition" description-key="Property_Style2TopPosition" of-type="SingleLine.Text" usage="bound" required="true" default-value="300"/>
    <property name="Style2LeftPosition" display-name-key="Property_Style2LeftPosition" description-key="Property_Style2LeftPosition" of-type="SingleLine.Text" usage="bound" required="true" default-value="24"/>
    <resources>
      <code path="index.ts" order="1"/>
      <css path="css/AnnualReport.css" order="1" />
    </resources>
  </control>
</manifest>
